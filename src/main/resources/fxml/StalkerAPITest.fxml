                        </HBox>
                        <HBox alignment="CENTER" spacing="5.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Online:"/>
                                <Text fx:id="lOnline" strokeType="OUTSIDE" strokeWidth="0.0"/>
                            </children>
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </padding>
                        </HBox>
                        <HBox alignment="CENTER" spacing="5.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="last active"/>
                                <Text fx:id="lLastActive" strokeType="OUTSIDE" strokeWidth="0.0"/>
                            </children>
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </padding>
                        </HBox>
                        <HBox alignment="CENTER" spacing="5.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="version"/>
