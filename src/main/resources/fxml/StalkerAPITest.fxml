                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="HTTP_METHOD"/>
                <TextField fx:id="tMethod" GridPane.columnIndex="1"/>
                <ComboBox fx:id="cmbUsers" prefWidth="150.0" GridPane.rowIndex="1"/>
                <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <children>
                        <HBox alignment="CENTER" spacing="5.0" VBox.vgrow="NEVER">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="LOGIN:"/>
                                <Text fx:id="lLogin" strokeType="OUTSIDE" strokeWidth="0.0"/>
                            </children>
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </padding>
                        </HBox>
                        <HBox alignment="CENTER" spacing="5.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="MAC"/>
                                <Text fx:id="lMac" strokeType="OUTSIDE" strokeWidth="0.0"/>
                            </children>
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </padding>
                        </HBox>
                        <HBox alignment="CENTER" spacing="5.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="ip"/>
                                <Text fx:id="lIP" strokeType="OUTSIDE" strokeWidth="0.0"/>
                            </children>
