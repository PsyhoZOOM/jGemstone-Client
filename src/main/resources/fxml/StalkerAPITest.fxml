                <HBox prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="2">
                    <children>
                        <VBox prefHeight="200.0" prefWidth="100.0">
                            <children>
                                <Button fx:id="bGet" mnemonicParsing="false" onAction="#getAction" text="GET TARRIFS"/>
                                <Button fx:id="bGetUsers" mnemonicParsing="false" onAction="#getUsers"
