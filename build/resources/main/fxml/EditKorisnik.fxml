<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTabPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.layout.AnchorPane?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="1.7976931348623157E308"
  maxWidth="1.7976931348623157E308" prefHeight="800.0" prefWidth="1280.0"
  xmlns="http://javafx.com/javafx/8.0.172"
  fx:controller="net.yuvideo.jgemstone.client.Controllers.EditKorisnikController">
  <children>
    <JFXTabPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" fx:id="tabKorisnikEdit">
      <tabs>
        <Tab closable="false" text="Korisnički podaci" fx:id="tabKorisnickiPodaci">
          <content>
            <AnchorPane fx:id="anchroKorsinikPodaci">
              <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
              </padding>
            </AnchorPane>
          </content>
          <graphic>
            <FontIcon fill="white" iconLiteral="eli-user" iconSize="25"/>
          </graphic>
        </Tab>
        <Tab closable="false" onSelectionChanged="#refreshUgovori" text="Ugovori"
          fx:id="tabKorisnikUgovori">
          <content>
            <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
              fx:id="anchorKorisnikUgovori">
              <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
              </padding>
            </AnchorPane>
          </content>
          <graphic>
            <FontIcon fill="white" iconLiteral="eli-list" iconSize="25"/>
          </graphic>
        </Tab>
        <Tab closable="false" onSelectionChanged="#refreshUgovori" text="Servisi"
          fx:id="tabKorisnikUsluge">
          <content>
            <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
              fx:id="anchorKorisnikUsluge">
              <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
              </padding>
            </AnchorPane>
          </content>
          <graphic>
            <FontIcon fill="white" iconLiteral="eli-list" iconSize="25"/>
          </graphic>
        </Tab>
        <Tab closable="false" text="Oprema" fx:id="tabOprema">
          <content>
            <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
              fx:id="anchorKorisnikOprema">
              <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
              </padding>
            </AnchorPane>
          </content>
          <graphic>
            <FontIcon fill="white" iconLiteral="mdi-settings" iconSize="25"/>
          </graphic>
        </Tab>
        <Tab onSelectionChanged="#refreshUplate" text="Uplate" fx:id="tabUplate">
          <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
          </content>
          <graphic>
            <FontIcon iconColor="WHITE" iconLiteral="mdi-currency-eur" iconSize="25"/>
          </graphic>
        </Tab>
        <Tab text="Zaduženja" fx:id="tabZaduzenja">
          <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
          </content>
          <graphic>
            <FontIcon iconColor="WHITE" iconLiteral="mdi-format-list-bulleted"/>
          </graphic>
        </Tab>
      </tabs>
    </JFXTabPane>
  </children>
</AnchorPane>
